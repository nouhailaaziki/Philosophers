./philo_bonus 1 800 200 200                              die
./philo_bonus 4 310 200 100                              die
./philo_bonus 4 310 100 200                              live
./philo_bonus 4 410 200 200                              live
./philo_bonus 50 400 200 200                             die
./philo_bonus 100 800 200 200                            live
./philo_bonus 5 310 100 200                              live
./philo_bonus 5 310 200 100                              die
./philo_bonus 5 800 200 200                              live
./philo_bonus 5 800 200 200 7 | grep eating | wc -l      live
